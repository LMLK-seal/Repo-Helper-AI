# This is an example file. Copy it to .env and fill in your own values.
# DO NOT commit your .env file to Git.

# Get from GitHub Developer Settings -> Personal Access Tokens (classic)
# Needs "repo" scope.
GITHUB_TOKEN="ghp_YourGitHubTokenHere"

# Get from Google AI Studio -> Get API Key
GEMINI_API_KEY="YourGoogleGeminiApiKeyHere"

# A random, secret string you create for securing your webhook
GITHUB_WEBHOOK_SECRET="your_strong_random_secret_here"

# Your GitHub username and the repo name
REPO_OWNER="your-github-username"
REPO_NAME="your-repo-name"